#: venv/lib/python3.7/site-packages/django/contrib/flatpages/admin.py:12
msgid "Advanced options"
msgstr "Opciones avanzadas"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/apps.py:7
msgid "Flat Pages"
msgstr "Páginas estáticas"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:9
#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:12
msgid ""
"Example: '/about/contact/'. Make sure to have leading and trailing slashes."
msgstr ""
"Ejemplo: '/about/contact/'. Asegúrese de que pone barras al principio y al "
"final."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Este valor solo puede contener letras, números, puntos, guiones bajos o "
"medios, barras o tildes."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:29
msgid "URL is missing a leading slash."
msgstr "A la URL le falta la barra inicial."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:36
msgid "URL is missing a trailing slash."
msgstr "A la URL le falta la barra final."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/forms.py:53
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr " En el sitio %(site)s ya hay un Flatpage con la url %(url)s"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "título"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "contenido"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "habilitar comentarios"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nombre de plantilla"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: 'flatpages/contact_page.html'. If this isn't provided, the system "
"will use 'flatpages/default.html'."
msgstr ""
"Ejemplo: 'flatpages/contact_page.html'. Si no se proporciona uno, el sistema "
"usará 'flatpages/default.html'."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "debe estar registrado"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:24
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr "Si está marcado, sólo los usuarios registrados podrán ver la página."

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:27
msgid "sites"
msgstr "sitios"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:31
msgid "flat page"
msgstr "página estática"

#: venv/lib/python3.7/site-packages/django/contrib/flatpages/models.py:32
msgid "flat pages"
msgstr "páginas estáticas"
